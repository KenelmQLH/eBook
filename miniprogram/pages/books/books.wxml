<!--pages/demo-search/demo-search.wxml-->
<import src="/wxSearch/wxSearch.wxml"/>
<view class="wxSearch-section">
      <view class="wxSearch-pancel">
          <input bindinput="wxSearchInput" bindfocus="wxSearchFocus" value="{{wxSearchData.value}}" bindblur="wxSearchBlur" class="wxSearch-input" placeholder="书籍搜索" />
          <button class="wxSearch-button" bindtap="wxSearchFn" size="mini" plain="true">搜索</button>
      </view>
</view>
<template is="wxSearch" data="{{wxSearchData}}"/>
<view class='category_list' wx:if="{{!onsearch}}">
    <view>
        <image src='../../images/计算机.png' class='icon_imag'></image>
        <text class='icon_name'>计算机科学与技术</text>
    </view>
    <view>
        <image src='../../images/网络空间安全.png' class='icon_imag'></image>
        <text class='icon_name' style='position:relative; left:10px'>网络空间安全</text>
    </view>
    <view>
        <image src='../../images/智能科学与技术.png' class='icon_imag'></image>
        <text class='icon_name' style='position:relative; left:3px'>智能科学与技术</text>
    </view>
    <view style='position:relative;top:15px'>
        <image src='../../images/通信.png' class='icon_imag'></image>
        <text class='icon_name'style='position:relative; left:2px'>信息与通信工程</text>
    </view>
    <view style='position:relative;top:15px'>
        <image src='../../images/公共.png' class='icon_imag'></image>
        <text class='icon_name'style='position:relative; left:15px'>公共/通修</text>
    </view>
    <view style='position:relative;top:15px'>
        <image src='../../images/其他.png' class='icon_imag'></image>
        <text class='icon_name' style='position:relative; left:30px'>其他</text>
    </view>
</view>



