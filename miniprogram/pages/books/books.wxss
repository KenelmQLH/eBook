/* pages/demo-search/demo-search.wxss*/ 
@import "/wxSearch/wxSearch.wxss";


.container {
  width: 750rpx;
  height:100vh;
  position:relative;
  top:0;
  /*
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
*/
  padding: 0;
  box-sizing: border-box;
} 

.adjust{
  width:750rpx;
}

.category_list{
  margin: 60rpx;
  position: relative;
  top: 15vh;
  
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  flex-wrap: wrap;
}
.forborder{
  margin: 0;
  line-height: 8vh;
  text-align: center;
}
.icon_imag{
  position: relative;
  margin: 0;
  display: flex;
  width: 150rpx;
  height: 150rpx;
  justify-content: center;
  align-items: center;
}

.icon_name{
  position: relative;
  top: 0;
  left: 0;
  font-size: 12px;
  font-weight:400;
}
/*------------------*/
.bottom{
  position:absolute;
  bottom:0;
  margin-bottom:0;
  width:750rpx;
  
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
 


